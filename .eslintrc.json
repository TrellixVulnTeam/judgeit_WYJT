{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "arrow-parens": ["error", "as-needed"],
    "consistent-return": "off",
    "space-before-function-paren": "off",
    "function-paren-newline": "off",
    "comma-dangle": ["error", "never"]
  }
}
